{
    "pageSize": 10,
    "request": {
        "type": "GET",
        "api": "/api/getUsers",
        "parameters": [],
        "fields": "",
        "root": ""
    },
    "paging": {
        "enable": true,
        "settings": {
            "pageSize": 10
        }
    },
    "sorting": {
        "enable": true
    },
    "grouping": {
        "enable": true,
        "group": []
    },
    "filtering": {
        "enable": false
    },
    "customGrouping": false,
    "allowResizing": true,
    "reorderable": true,
    "selectable": true,
    "scrollable": false,
    "hideable": true,
    "refresh": false,
    "reset": true,
    "contextMenuItems": [
        "AutoFit",
        "AutoFitAll",
        "SortAscending",
        "SortDescending",
        "Copy",
        "Edit",
        "Delete",
        "Save",
        "Cancel",
        "FirstPage",
        "PrevPage",
        "LastPage",
        "NextPage",
        "Group",
        "Ungroup"
    ],
    "toolbar": [
        "Add",
        "Edit",
        "Delete",
        "Print",
        "Search"
    ],
    "editSettings": {
        "allowEditing": true,
        "allowAdding": true,
        "allowDeleting": true,
        "showDeleteConfirmDialog": true,
        "mode": "Dialog"
    },
    "editSettingsRequest": {
        "add": {
            "type": "POST",
            "api": "/api/createUser",
            "parameters": [],
            "fields": "",
            "root": ""
        },
        "edit": {
            "type": "POST",
            "api": "/api/updateUser",
            "parameters": [],
            "fields": "",
            "root": ""
        },
        "delete": {
            "type": "POST",
            "api": "/api/deleteUser",
            "parameters": [],
            "fields": "",
            "root": ""
        }
    },
    "columns": [
        {
            "title": "ID",
            "width": 70,
            "filterable": true,
            "hideable": true,
            "filter": "date",
            "type": "number",
            "class": "text-align-right",
            "field": "id",
            "isPrimaryKey": true,
            "allowEditing": false
        },
        {
            "title": "Firstname",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "firstname",
            "validation": {
                "required": true
            }
        },
        {
            "title": "Phone",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "phone",
            "validation": {
                "required": true
            }
        },
        {
            "title": "Mobile",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "mobile",
            "validation": {
                "required": true
            }
        },
        {
            "title": "Address",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "address",
            "validation": {
                "required": true
            }
        },
        {
            "title": "Zip code",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "zip_code",
            "validation": {
                "required": true
            }
        },
        {
            "title": "City",
            "filterable": true,
            "hideable": true,
            "type": "string",
            "class": "text-align-right",
            "field": "city",
            "validation": {
                "required": true
            }
        },
        {
            "title": "Active",
            "template": {
                "choices": [
                    {
                        "field": "active",
                        "value": 0,
                        "operator": "==",
                        "html": "<span class='e-badge e-badge-danger'>Inactive</span>"
                    },
                    {
                        "field": "active",
                        "value": 1,
                        "operator": "==",
                        "html": "<span class='e-badge e-badge-success'>Active</span>"
                    },
                    {
                        "field": "active",
                        "value": 2,
                        "operator": "==",
                        "html": "<span class='e-badge e-badge-warning'>Verified mail</span>"
                    }
                ]
            },
            "field": "active"
        },
        {
            "title": "Club",
            "template": {
                "choices": [
                    {
                        "field": "is_club",
                        "value": 1,
                        "operator": "==",
                        "html": "<span class='e-badge e-badge-info'>Club</span>"
                    }
                ]
            },
            "field": "active"
        }
    ],
    "config": [
        {
            "type": "textbox",
            "width": "col-md-12 hide",
            "class": "e-outline",
            "name": "id",
            "title": "ID",
            "field": "id",
            "readonly": true
        },
        {
            "type": "textbox",
            "width": "col-md-12",
            "class": "e-outline",
            "name": "firstname",
            "title": "Firstname",
            "field": "firstname"
        },
        {
            "type": "textbox",
            "width": "col-md-6",
            "class": "e-outline",
            "name": "email",
            "title": "Email",
            "field": "email"
        },
        {
            "type": "password",
            "width": "col-md-6",
            "class": "e-outline",
            "name": "password",
            "title": "Password",
            "field": "password",
            "readonly": true
        },
        {
            "type": "textbox",
            "width": "col-md-6",
            "class": "e-outline",
            "name": "phone",
            "title": "Phone",
            "field": "phone"
        },
        {
            "type": "textbox",
            "width": "col-md-6",
            "class": "e-outline",
            "name": "mobile",
            "title": "Mobile",
            "field": "mobile"
        },
        {
            "type": "textbox",
            "width": "col-md-4",
            "class": "e-outline",
            "name": "address",
            "title": "Address",
            "field": "address"
        },
        {
            "type": "textbox",
            "width": "col-md-4",
            "class": "e-outline",
            "name": "zip_code",
            "title": "Zip code",
            "field": "zip_code"
        },
        {
            "type": "textbox",
            "width": "col-md-4",
            "class": "e-outline",
            "name": "city",
            "title": "City",
            "field": "city"
        },
        {
            "type": "switch",
            "width": "col-md-12 mt-2",
            "class": "e-outline",
            "name": "active",
            "title": "Active",
            "field": "active"
        },
        {
            "type": "switch",
            "width": "col-md-12",
            "class": "e-outline",
            "name": "is_club",
            "title": "Is club?",
            "field": "is_club"
        },
        {
            "type": "button",
            "width": "col-md-12",
            "class": "e-info button-action",
            "name": "submit",
            "title": "Save",
            "field": "submit",
            "positionClass": "position-end mt-3 col-md-12"
        }
    ]
}