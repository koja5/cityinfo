{
  "name": "city-info",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "client": "ng serve --port 4201 --proxy-config proxy.config.json",
    "server": "nodemon ../server/server.js --port 3001",
    "start": "npx npm-run-all -p client server",
    "build": "ng serve --port 3000 --open && nodemon server",
    "build-compress": "ng build --aot && gzipper c ./dist --include js,css,html --output-file-format [filename].[ext] ./dist-compressed/",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "node --max-old-space-size=8192 node_modules/@angular/cli/bin/ng run CityInfo:serve-ssr",
    "serve:ssr": "node dist/CityInfo/server/main.js",
    "build:ssr": "ng build && ng run CityInfo:server",
    "prerender": "ng run CityInfo:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/platform-server": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@nguniversal/express-engine": "^14.2.3",
    "@syncfusion/ej2": "^21.2.10",
    "@syncfusion/ej2-angular-buttons": "^21.2.10",
    "@syncfusion/ej2-angular-calendars": "^21.2.10",
    "@syncfusion/ej2-angular-charts": "^21.2.10",
    "@syncfusion/ej2-angular-dropdowns": "^21.2.10",
    "@syncfusion/ej2-angular-grids": "^21.2.10",
    "@syncfusion/ej2-angular-inputs": "^21.2.9",
    "@syncfusion/ej2-angular-navigations": "^21.2.9",
    "@syncfusion/ej2-angular-pdfviewer": "^21.2.10",
    "@syncfusion/ej2-angular-popups": "^21.2.10",
    "@syncfusion/ej2-angular-schedule": "^21.2.10",
    "@syncfusion/ej2-angular-splitbuttons": "^21.2.5",
    "@syncfusion/ej2-base": "^21.2.9",
    "@syncfusion/ej2-schedule": "^21.2.10",
    "@types/stripe-v3": "^3.1.28",
    "angular2-uuid": "^1.1.1",
    "bootstrap": "^4.5.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "http-proxy-middleware": "^2.0.6",
    "i": "^0.3.7",
    "localstorage-polyfill": "^1.0.1",
    "ng-lazyload-image": "^9.1.3",
    "ngx-cookie-service": "^15.0.0",
    "ngx-image-compress": "^15.1.5",
    "ngx-image-cropper": "^6.3.4",
    "ngx-skeleton-loader": "^5.0.0",
    "ngx-toastr": "^13.2.1",
    "npm": "^9.6.0",
    "rxjs": "~7.5.0",
    "stripe-angular": "^1.9.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@nguniversal/builders": "^14.2.3",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "^4.8.4"
  }
}
