{
  "name": "city-info",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "client": "ng serve --port 4201 --proxy-config proxy.config.json",
    "server": "nodemon ../server/server.js --port 3001",
    "start": "npx npm-run-all -p client server",
    "build": "ng serve --port 3000 --open && nodemon server",
    "build-compress": "ng build --aot && gzipper c ./dist --include js,css,html --output-file-format [filename].[ext] ./dist-compressed/",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
